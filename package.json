  "scripts": {
    "bootstrap": "lerna bootstrap && npm install",
    "website": "npx dumi dev",
    "build": "lerna exec --parallel -- babel src -d dist --ignore src/__tests__/ --extensions .ts,.tsx,.js",
    "build:website": "webpack --config webpack.config.js",
    "tsc": "lerna run tsc --parallel",
    "clean": "lerna exec --parallel -- rimraf -rf ./dist",
    "prepublish": "npm run clean && cp -r packages/kzui/src packages/kzui/dist && npm run build && npm run tsc && npm run style",
    "publish": "lerna publish",
    "style": "cross-env BUILD_MODE=style npm run build:website && mv public/dist/styles.css packages/kzui/dist",
    "docs:build": "dumi build && node gen-site-404.js",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy"
  },
  "dependencies": {
    "@kzui/utils": "^0.2.4",
